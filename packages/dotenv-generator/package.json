{
  "name": "dotenv-generator",
  "private": true,
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "lintJson": "eslint --no-eslintrc --config .eslintrc.json.js --ext .json --fix .",
    "lintSrc": "eslint --ext .js,.jsx,.ts,.tsx --fix .",
    "lint": "run-p lintJson lintSrc",
    "fix": "prettier --loglevel warn --write .",
    "lix": "run-p lint fix"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-json": "^3.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "typescript": "^4.3.2"
  }
}

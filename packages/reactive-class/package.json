{
  "name": "reactive-class",
  "private": true,
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "lintJson": "eslint --no-eslintrc --config .eslintrc.json.js --ext .json --fix .",
    "lintSrc": "eslint --ext .js,.jsx,.ts,.tsx --fix .",
    "lint": "run-p lintJson lintSrc",
    "fix": "prettier --loglevel warn --write .",
    "lix": "run-p lint fix",
    "start": "yarn build && node scripts/index.js",
    "codegen": "index-generator generate",
    "build": "yarn codegen && tsc",
    "prepublish": "yarn build",
    "test": "yarn jest"
  },
  "dependencies": {
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.11",
    "@types/react-test-renderer": "^17.0.1",
    "autobind-decorator": "^2.4.0",
    "default-ts-configs": "workspace:packages/default-ts-configs",
    "eslint": "^7.28.0",
    "index-generator": "workspace:packages/index-generator",
    "jest": "^27.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "react": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "rxjs": "^7.1.0",
    "typescript": "^4.3.2"
  }
}

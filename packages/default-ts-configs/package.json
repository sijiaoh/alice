{
  "name": "default-ts-configs",
  "private": true,
  "engines": {
    "node": "14.x"
  },
  "bin": "scripts/index.js",
  "scripts": {
    "lintJson": "eslint --no-eslintrc --config .eslintrc.json.js --ext .json --fix .",
    "lintSrc": "eslint --ext .js,.jsx,.ts,.tsx --fix .",
    "lint": "run-p lintJson lintSrc",
    "fix": "prettier --loglevel warn --write .",
    "lix": "run-p lint fix",
    "start": "yarn build && node scripts/index.js",
    "build": "tsc",
    "prepublish": "yarn build",
    "test": "yarn jest"
  },
  "dependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.2",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "commander": "^7.2.0",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-json": "^3.0.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.2"
  }
}

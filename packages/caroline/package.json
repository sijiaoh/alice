{
  "name": "caroline",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "generateIndex": "index-generator generate",
    "generateGraphqlSchema": "NODE_PATH=${INIT_CWD} ts-node scripts/generate-graphql-schema.ts",
    "codegen": "run-p generateIndex generateGraphqlSchema",
    "dev": "yarn codegen && next dev",
    "build": "yarn codegen && next build",
    "start": "next start",
    "prepareDatabase": "mysql-docker prepare caroline caroline caroline-password",
    "prepublish": "run-s codegen prepareDatabase"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "apollo-server-micro": "^2.25.2",
    "autobind-decorator": "^2.4.0",
    "graphql": "^15.5.1",
    "index-generator": "workspace:packages/index-generator",
    "mysql-docker": "workspace:packages/mysql-docker",
    "next": "11.0.1",
    "npm-run-all": "^4.1.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.1.0",
    "type-graphql": "^1.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "eslint": "7.30.0"
  }
}
